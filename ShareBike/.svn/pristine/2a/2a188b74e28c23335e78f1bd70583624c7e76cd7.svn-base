public class RegexUtil {
    /**
     * 最少10位数字+字母
     */
    public static String regex1 = "^[a-zA-Z0-9]{10,}$";

    public static boolean isStringNumTen(CharSequence input) {
        return isMatch(regex1, input);
    }
}

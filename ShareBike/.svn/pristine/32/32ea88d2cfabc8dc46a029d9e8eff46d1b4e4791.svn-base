<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="@dimen/space_dimens_8"
    android:background="@color/bg_white"
    android:orientation="vertical"
    tools:context="com.panda.sharebike.ui.userguide.RegisterAndAccountActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/space_dimens_8"
        android:layout_marginBottom="@dimen/space_dimens_8"
        android:text="@string/register_account_one"
        android:textSize="@dimen/text_dimens_14"
        android:textColor="@color/tv_selected_color" />

    <ImageView
        android:layout_gravity="center"
        android:scaleType="fitXY"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/icon_realname" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/space_dimens_8"
        android:text="@string/reguster_account_two"
        android:textSize="@dimen/text_dimens_14"
        android:textColor="@color/tv_selected_color" />
</LinearLayout>

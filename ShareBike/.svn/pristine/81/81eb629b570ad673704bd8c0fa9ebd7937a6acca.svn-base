<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal">

    <LinearLayout
        android:paddingLeft="@dimen/dp_6"
        android:background="@color/bg_white"
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="match_parent">

        <View
            android:layout_gravity="center"
            android:layout_width="1px"
            android:layout_height="15dp"
            android:background="@color/order_bt_color_end" />

        <ImageView
            android:src="@drawable/icon_time_axis"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <View
            android:layout_gravity="center"
            android:layout_width="1px"
            android:layout_height="match_parent"
            android:background="@color/order_bt_color_end" />

    </LinearLayout>
    <RelativeLayout
        android:id="@+id/rl_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/bg_white">

        <TextView
            android:id="@+id/tv_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginTop="@dimen/space_dimens_12"
            android:textColor="#FF333333"
            android:paddingLeft="@dimen/margin_horizontal"
            android:paddingRight="@dimen/margin_horizontal"
            android:textSize="@dimen/text_dimens_15"
            tools:text="2017-09-25  09:44:20" />

        <LinearLayout
            android:layout_below="@+id/tv_time"
            android:id="@+id/ll_layout2"
            android:paddingLeft="@dimen/margin_horizontal"
            android:paddingRight="@dimen/margin_horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/tv_number1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tv_time"
                android:layout_marginTop="@dimen/space_dimens_12"
                android:textColor="#FF666666"
                android:textSize="12sp"
                android:text="单车编号：" />

            <TextView
                android:id="@+id/tv_number"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tv_time"
                android:layout_marginTop="@dimen/space_dimens_12"
                android:textColor="#FF666666"
                android:textSize="12sp"
                tools:text="0000001" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/ll_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/ll_layout2"
            android:paddingLeft="@dimen/margin_horizontal"
            android:paddingRight="@dimen/margin_horizontal"
            android:layout_marginTop="@dimen/space_dimens_12">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="花费：¥"
                android:textColor="#FF666666"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/tv_money"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="1.00"
                android:textColor="#FF666666"
                android:textSize="12sp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="20dp"
                android:text="时间："
                android:textColor="#FF666666"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/tv_play_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:text="5分钟"
                android:textColor="#FF666666"
                android:textSize="12sp" />
        </LinearLayout>

        <ImageView
            android:id="@+id/iv_right_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/activity_horizontal_margin"
            android:padding="3dp"
            android:scaleType="fitCenter"
            android:src="@drawable/icon_right" />

        <View
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:layout_below="@+id/ll_layout"
            android:layout_marginTop="@dimen/space_dimens_12"
            android:background="@color/bg_view" />
    </RelativeLayout>


</LinearLayout>
<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="@dimen/space_dimens_8"
    android:background="@color/bg_white"
    tools:context="com.panda.sharebike.ui.userguide.BookingAndUnlockQuestionActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/space_dimens_8"
        android:textColor="@color/tv_bold_color"
        android:textSize="@dimen/text_dimens_14"
        android:text="@string/order_title" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/space_dimens_8"
        android:text="@string/order_content"
        android:textSize="@dimen/text_dimens_14"
        android:textColor="@color/tv_selected_color" />
</LinearLayout>

        android:layout_width="152dp"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/item_order_detail_result_description_tvValue"
        android:textSize="14sp"
        android:textColor="@color/txt333"
        android:text="60万"
        android:layout_width="152dp"
        android:layout_height="wrap_content" />

</LinearLayout>